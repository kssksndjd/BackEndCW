{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nexport default {\n  data() {\n    return {\n      searchQuery: \"\",\n      sortKey: \"subject\",\n      sortOrder: \"asc\",\n      showCart: false,\n      lessons: [] // Add 'imageUrl' in the lesson objects from the API\n    };\n  },\n  methods: {\n    async fetchLessons() {\n      const response = await fetch(\"http://localhost:3000/collections/lessons\");\n      this.lessons = await response.json();\n    },\n    addToCart(lesson) {\n      if (lesson.spaces > 0) {\n        this.$emit(\"add-to-cart\", lesson);\n        lesson.spaces--;\n      }\n    },\n    togglePage() {\n      this.showCart = !this.showCart;\n    },\n    toggleSortOrder() {\n      this.sortOrder = this.sortOrder === \"asc\" ? \"desc\" : \"asc\";\n    }\n  },\n  computed: {\n    filteredAndSortedLessons() {\n      let filtered = this.lessons.filter(lesson => {\n        const query = this.searchQuery.toLowerCase();\n        return lesson.subject.toLowerCase().includes(query) || lesson.location.toLowerCase().includes(query) || lesson.price.toString().includes(query) || lesson.spaces.toString().includes(query);\n      });\n      return filtered.sort((a, b) => {\n        let result;\n        if (this.sortKey === \"price\" || this.sortKey === \"spaces\") {\n          result = a[this.sortKey] - b[this.sortKey];\n        } else {\n          result = a[this.sortKey].localeCompare(b[this.sortKey]);\n        }\n        return this.sortOrder === \"asc\" ? result : -result;\n      });\n    }\n  },\n  mounted() {\n    this.fetchLessons();\n  }\n};","map":{"version":3,"names":["data","searchQuery","sortKey","sortOrder","showCart","lessons","methods","fetchLessons","response","fetch","json","addToCart","lesson","spaces","$emit","togglePage","toggleSortOrder","computed","filteredAndSortedLessons","filtered","filter","query","toLowerCase","subject","includes","location","price","toString","sort","a","b","result","localeCompare","mounted"],"sources":["/Users/lyphilong/Desktop/FullStack/project/src/components/LessonList.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-5\">\n    <h1 class=\"text-center mb-4\">After School Classes</h1>\n\n    <button class=\"btn btn-info mb-4\" @click=\"togglePage\">\n      {{ showCart ? 'Back to Lessons' : 'View Shopping Cart' }}\n    </button>\n\n    <div v-if=\"!showCart\">\n      <!-- Search Input -->\n      <div class=\"form-group\">\n        <label for=\"searchQuery\">Search Lessons:</label>\n        <input\n          type=\"text\"\n          id=\"searchQuery\"\n          v-model=\"searchQuery\"\n          class=\"form-control\"\n          placeholder=\"Search by subject, location, price, or spaces\"\n        />\n      </div>\n\n      <!-- Sort Dropdown -->\n      <div class=\"form-group\">\n        <label for=\"sortKey\">Sort By:</label>\n        <select v-model=\"sortKey\" id=\"sortKey\" class=\"form-control\">\n          <option value=\"subject\">Subject</option>\n          <option value=\"location\">Location</option>\n          <option value=\"price\">Price</option>\n          <option value=\"spaces\">Spaces</option>\n        </select>\n        <button class=\"btn btn-secondary mt-2\" @click=\"toggleSortOrder\">\n          Sort: {{ sortOrder === 'asc' ? 'Ascending' : 'Descending' }}\n        </button>\n      </div>\n\n      <!-- Display Lessons -->\n      <div class=\"row\">\n        <div class=\"col-md-4\" v-for=\"lesson in filteredAndSortedLessons\" :key=\"lesson._id\">\n          <div class=\"card mb-4\">\n            <img :src=\"lesson.imageUrl\" class=\"card-img-top\" alt=\"Lesson Image\" />\n            <div class=\"card-body\">\n              <h5 class=\"card-title\">\n                <i class=\"fas fa-chalkboard-teacher\"></i>\n                {{ lesson.subject }}\n              </h5>\n              <p class=\"card-text\">Location: {{ lesson.location }}</p>\n              <p class=\"card-text\">Price: ${{ lesson.price }}</p>\n              <p class=\"card-text\">Spaces Available: {{ lesson.spaces }}</p>\n              <button\n                class=\"btn btn-primary\"\n                :disabled=\"lesson.spaces === 0\"\n                @click=\"addToCart(lesson)\"\n              >\n                {{ lesson.spaces === 0 ? 'Sold Out' : 'Add to Cart' }}\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      searchQuery: \"\",\n      sortKey: \"subject\",\n      sortOrder: \"asc\",\n      showCart: false,\n      lessons: [], // Add 'imageUrl' in the lesson objects from the API\n    };\n  },\n  methods: {\n    async fetchLessons() {\n      const response = await fetch(\"http://localhost:3000/collections/lessons\");\n      this.lessons = await response.json();\n    },\n    addToCart(lesson) {\n      if (lesson.spaces > 0) {\n        this.$emit(\"add-to-cart\", lesson);\n        lesson.spaces--;\n      }\n    },\n    togglePage() {\n      this.showCart = !this.showCart;\n    },\n    toggleSortOrder() {\n      this.sortOrder = this.sortOrder === \"asc\" ? \"desc\" : \"asc\";\n    },\n  },\n  computed: {\n    filteredAndSortedLessons() {\n      let filtered = this.lessons.filter((lesson) => {\n        const query = this.searchQuery.toLowerCase();\n        return (\n          lesson.subject.toLowerCase().includes(query) ||\n          lesson.location.toLowerCase().includes(query) ||\n          lesson.price.toString().includes(query) ||\n          lesson.spaces.toString().includes(query)\n        );\n      });\n      return filtered.sort((a, b) => {\n        let result;\n        if (this.sortKey === \"price\" || this.sortKey === \"spaces\") {\n          result = a[this.sortKey] - b[this.sortKey];\n        } else {\n          result = a[this.sortKey].localeCompare(b[this.sortKey]);\n        }\n        return this.sortOrder === \"asc\" ? result : -result;\n      });\n    },\n  },\n  mounted() {\n    this.fetchLessons();\n  },\n};\n</script>\n"],"mappings":";;AAgEA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,OAAO,EAAE,SAAS;MAClBC,SAAS,EAAE,KAAK;MAChBC,QAAQ,EAAE,KAAK;MACfC,OAAO,EAAE,EAAE,CAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAAC,2CAA2C,CAAC;MACzE,IAAI,CAACJ,OAAM,GAAI,MAAMG,QAAQ,CAACE,IAAI,CAAC,CAAC;IACtC,CAAC;IACDC,SAASA,CAACC,MAAM,EAAE;MAChB,IAAIA,MAAM,CAACC,MAAK,GAAI,CAAC,EAAE;QACrB,IAAI,CAACC,KAAK,CAAC,aAAa,EAAEF,MAAM,CAAC;QACjCA,MAAM,CAACC,MAAM,EAAE;MACjB;IACF,CAAC;IACDE,UAAUA,CAAA,EAAG;MACX,IAAI,CAACX,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;IAChC,CAAC;IACDY,eAAeA,CAAA,EAAG;MAChB,IAAI,CAACb,SAAQ,GAAI,IAAI,CAACA,SAAQ,KAAM,KAAI,GAAI,MAAK,GAAI,KAAK;IAC5D;EACF,CAAC;EACDc,QAAQ,EAAE;IACRC,wBAAwBA,CAAA,EAAG;MACzB,IAAIC,QAAO,GAAI,IAAI,CAACd,OAAO,CAACe,MAAM,CAAER,MAAM,IAAK;QAC7C,MAAMS,KAAI,GAAI,IAAI,CAACpB,WAAW,CAACqB,WAAW,CAAC,CAAC;QAC5C,OACEV,MAAM,CAACW,OAAO,CAACD,WAAW,CAAC,CAAC,CAACE,QAAQ,CAACH,KAAK,KAC3CT,MAAM,CAACa,QAAQ,CAACH,WAAW,CAAC,CAAC,CAACE,QAAQ,CAACH,KAAK,KAC5CT,MAAM,CAACc,KAAK,CAACC,QAAQ,CAAC,CAAC,CAACH,QAAQ,CAACH,KAAK,KACtCT,MAAM,CAACC,MAAM,CAACc,QAAQ,CAAC,CAAC,CAACH,QAAQ,CAACH,KAAK;MAE3C,CAAC,CAAC;MACF,OAAOF,QAAQ,CAACS,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;QAC7B,IAAIC,MAAM;QACV,IAAI,IAAI,CAAC7B,OAAM,KAAM,OAAM,IAAK,IAAI,CAACA,OAAM,KAAM,QAAQ,EAAE;UACzD6B,MAAK,GAAIF,CAAC,CAAC,IAAI,CAAC3B,OAAO,IAAI4B,CAAC,CAAC,IAAI,CAAC5B,OAAO,CAAC;QAC5C,OAAO;UACL6B,MAAK,GAAIF,CAAC,CAAC,IAAI,CAAC3B,OAAO,CAAC,CAAC8B,aAAa,CAACF,CAAC,CAAC,IAAI,CAAC5B,OAAO,CAAC,CAAC;QACzD;QACA,OAAO,IAAI,CAACC,SAAQ,KAAM,KAAI,GAAI4B,MAAK,GAAI,CAACA,MAAM;MACpD,CAAC,CAAC;IACJ;EACF,CAAC;EACDE,OAAOA,CAAA,EAAG;IACR,IAAI,CAAC1B,YAAY,CAAC,CAAC;EACrB;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}