{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nexport default {\n  props: ['lessons', 'showCart'],\n  data() {\n    return {\n      searchQuery: '',\n      sortKey: 'subject',\n      // Default sorting attribute\n      sortOrder: 'asc' // Default sorting order\n    };\n  },\n  methods: {\n    addToCart(lesson) {\n      if (lesson.spaces > 0) {\n        this.$emit('add-to-cart', lesson);\n        lesson.spaces--;\n      }\n    },\n    toggleSortOrder() {\n      this.sortOrder = this.sortOrder === 'asc' ? 'desc' : 'asc';\n    }\n  },\n  computed: {\n    filteredAndSortedLessons() {\n      let filtered = this.lessons.filter(lesson => {\n        const query = this.searchQuery.toLowerCase();\n        return lesson.subject.toLowerCase().includes(query) || lesson.location.toLowerCase().includes(query) || lesson.price.toString().includes(query) || lesson.spaces.toString().includes(query);\n      });\n\n      // Sort based on sortKey and sortOrder\n      return filtered.sort((a, b) => {\n        let result;\n        if (this.sortKey === 'price' || this.sortKey === 'spaces') {\n          result = a[this.sortKey] - b[this.sortKey];\n        } else {\n          result = a[this.sortKey].localeCompare(b[this.sortKey]);\n        }\n        return this.sortOrder === 'asc' ? result : -result;\n      });\n    }\n  },\n  mounted() {\n    this.fetchLessons();\n  }\n};","map":{"version":3,"names":["props","data","searchQuery","sortKey","sortOrder","methods","addToCart","lesson","spaces","$emit","toggleSortOrder","computed","filteredAndSortedLessons","filtered","lessons","filter","query","toLowerCase","subject","includes","location","price","toString","sort","a","b","result","localeCompare","mounted","fetchLessons"],"sources":["/Users/lyphilong/Desktop/FullStack/project/src/components/LessonList.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-5\">\n    <h1 class=\"text-center mb-4\">After School Classes</h1>\n\n    <!-- Toggle between Lesson and Cart Pages -->\n    <button class=\"btn btn-info mb-4\" @click=\"$emit('toggle-page')\">\n      {{ showCart ? 'Back to Lessons' : 'View Shopping Cart' }}\n    </button>\n\n    <div v-if=\"!showCart\">\n      <!-- Search Input -->\n      <div class=\"form-group\">\n        <label for=\"searchQuery\">Search Lessons:</label>\n        <input type=\"text\" id=\"searchQuery\" v-model=\"searchQuery\" class=\"form-control\" placeholder=\"Search by subject, location, price, or spaces\">\n      </div>\n\n      <!-- Sorting Options -->\n      <div class=\"form-group\">\n        <label for=\"sortKey\">Sort By:</label>\n        <select v-model=\"sortKey\" id=\"sortKey\" class=\"form-control\">\n          <option value=\"subject\">Subject</option>\n          <option value=\"location\">Location</option>\n          <option value=\"price\">Price</option>\n          <option value=\"spaces\">Spaces</option>\n        </select>\n\n        <!-- Sort Order Toggle -->\n        <button class=\"btn btn-secondary mt-2\" @click=\"toggleSortOrder\">\n          Sort: {{ sortOrder === 'asc' ? 'Ascending' : 'Descending' }}\n        </button>\n      </div>\n\n      <!-- Display Sorted Lessons -->\n      <div class=\"row\">\n        <div class=\"col-md-4\" v-for=\"lesson in filteredAndSortedLessons\" :key=\"lesson._id\">\n          <div class=\"card mb-4\">\n            <div class=\"card-body\">\n              <h5 class=\"card-title\">\n                <i class=\"fas fa-chalkboard-teacher\"></i> {{ lesson.subject }}\n              </h5>\n              <p class=\"card-text\">Location: {{ lesson.location }}</p>\n              <p class=\"card-text\">Price: ${{ lesson.price }}</p>\n              <p class=\"card-text\">Spaces Available: {{ lesson.spaces }}</p>\n              <button class=\"btn btn-primary\" :disabled=\"lesson.spaces === 0\" @click=\"addToCart(lesson)\">\n                {{ lesson.spaces === 0 ? 'Sold Out' : 'Add to Cart' }}\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: ['lessons', 'showCart'],\n  data() {\n    return {\n      searchQuery: '',\n      sortKey: 'subject',    // Default sorting attribute\n      sortOrder: 'asc',      // Default sorting order\n    };\n  },\n  methods: {\n    addToCart(lesson) {\n      if (lesson.spaces > 0) {\n        this.$emit('add-to-cart', lesson);\n        lesson.spaces--;\n      }\n    },\n    toggleSortOrder() {\n      this.sortOrder = this.sortOrder === 'asc' ? 'desc' : 'asc';\n    }\n  },\n  computed: {\n    filteredAndSortedLessons() {\n      let filtered = this.lessons.filter(lesson => {\n        const query = this.searchQuery.toLowerCase();\n        return lesson.subject.toLowerCase().includes(query) ||\n               lesson.location.toLowerCase().includes(query) ||\n               lesson.price.toString().includes(query) ||\n               lesson.spaces.toString().includes(query);\n      });\n\n      // Sort based on sortKey and sortOrder\n      return filtered.sort((a, b) => {\n        let result;\n        if (this.sortKey === 'price' || this.sortKey === 'spaces') {\n          result = a[this.sortKey] - b[this.sortKey];\n        } else {\n          result = a[this.sortKey].localeCompare(b[this.sortKey]);\n        }\n        return this.sortOrder === 'asc' ? result : -result;\n      });\n    }\n  },\n  mounted() {\n    this.fetchLessons();\n  }\n};\n</script>\n"],"mappings":";;AAuDA,eAAe;EACbA,KAAK,EAAE,CAAC,SAAS,EAAE,UAAU,CAAC;EAC9BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,OAAO,EAAE,SAAS;MAAK;MACvBC,SAAS,EAAE,KAAK,CAAO;IACzB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAACC,MAAM,EAAE;MAChB,IAAIA,MAAM,CAACC,MAAK,GAAI,CAAC,EAAE;QACrB,IAAI,CAACC,KAAK,CAAC,aAAa,EAAEF,MAAM,CAAC;QACjCA,MAAM,CAACC,MAAM,EAAE;MACjB;IACF,CAAC;IACDE,eAAeA,CAAA,EAAG;MAChB,IAAI,CAACN,SAAQ,GAAI,IAAI,CAACA,SAAQ,KAAM,KAAI,GAAI,MAAK,GAAI,KAAK;IA