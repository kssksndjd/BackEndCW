{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nexport default {\n  data() {\n    return {\n      lessons: []\n    };\n  },\n  methods: {\n    async fetchLessons() {\n      const response = await fetch('http://localhost:3000/collections/lessons');\n      this.lessons = await response.json();\n    },\n    addToCart(lesson) {\n      if (lesson.spaces > 0) {\n        this.$emit('add-to-cart', lesson);\n        lesson.spaces--;\n      }\n    }\n  },\n  computed: {\n    // Filter lessons based on search query\n    filteredAndSortedLessons() {\n      let filtered = this.lessons.filter(lesson => {\n        const query = this.searchQuery.toLowerCase();\n        return lesson.subject.toLowerCase().includes(query) || lesson.location.toLowerCase().includes(query) || lesson.price.toString().includes(query) || lesson.spaces.toString().includes(query);\n      });\n\n      // Sorting logic based on sortKey and sortOrder\n      return filtered.sort((a, b) => {\n        let result;\n        if (this.sortKey === 'price' || this.sortKey === 'spaces') {\n          result = a[this.sortKey] - b[this.sortKey];\n        } else {\n          result = a[this.sortKey].localeCompare(b[this.sortKey]);\n        }\n        return this.sortOrder === 'asc' ? result : -result;\n      });\n    },\n    isCheckoutValid() {\n      // Check if the name contains only letters and phone contains only numbers\n      const nameValid = /^[A-Za-z\\s]+$/.test(this.name);\n      const phoneValid = /^[0-9]+$/.test(this.phone);\n      return nameValid && phoneValid && this.cart.length > 0;\n    }\n  },\n  mounted() {\n    this.fetchLessons();\n  }\n};","map":{"version":3,"names":["data","lessons","methods","fetchLessons","response","fetch","json","addToCart","lesson","spaces","$emit","computed","filteredAndSortedLessons","filtered","filter","query","searchQuery","toLowerCase","subject","includes","location","price","toString","sort","a","b","result","sortKey","localeCompare","sortOrder","isCheckoutValid","nameValid","test","name","phoneValid","phone","cart","length","mounted"],"sources":["/Users/lyphilong/Desktop/FullStack/project/src/components/LessonList.vue"],"sourcesContent":["<template>\n  <!-- Add Bootstrap CDN for basic styling -->\n  <link rel=\"stylesheet\" href=\"https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css\">\n  <!-- Add Font Awesome CDN for icons -->\n  <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css\">\n\n    <div>\n      <h2>Available Lessons</h2>\n      <div v-for=\"lesson in lessons\" :key=\"lesson._id\">\n        <h3>{{ lesson.subject }}</h3>\n        <p>Location: {{ lesson.location }}</p>\n        <p>Price: ${{ lesson.price }}</p>\n        <p>Spaces Left: {{ lesson.spaces }}</p>\n        <button @click=\"addToCart(lesson)\" :disabled=\"lesson.spaces <= 0\">\n          Add to Cart\n        </button>\n      </div>\n    </div>\n    <div class=\"row\">\n        <div class=\"col-md-4\" v-for=\"lesson in filteredAndSortedLessons\" :key=\"lesson.id\">\n          <div class=\"card mb-4\">\n            <div class=\"card-body\">\n              <h5 class=\"card-title\">\n                <i class=\"fas fa-chalkboard-teacher\"></i> <!-- Font Awesome Icon -->\n                {{ lesson.subject }}\n              </h5>\n              <p class=\"card-text\">Location: {{ lesson.location }}</p>\n              <p class=\"card-text\">Price: ${{ lesson.price }}</p>\n              <p class=\"card-text\">Spaces Available: {{ lesson.spaces }}</p>\n              <button class=\"btn btn-primary\" :disabled=\"lesson.spaces === 0\" @click=\"addToCart(lesson)\">\n                {{ lesson.spaces === 0 ? 'Sold Out' : 'Add to Cart' }}\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n  </template>\n  \n  <script>\n  export default {\n    data() {\n      return {\n        lessons: []\n      };\n    },\n    methods: {\n      async fetchLessons() {\n        const response = await fetch('http://localhost:3000/collections/lessons');\n        this.lessons = await response.json();\n      },\n      addToCart(lesson) {\n        if (lesson.spaces > 0) {\n          this.$emit('add-to-cart', lesson);\n          lesson.spaces--;\n        }\n      }\n    },\n    computed: {\n        // Filter lessons based on search query\n        filteredAndSortedLessons() {\n          let filtered = this.lessons.filter(lesson => {\n            const query = this.searchQuery.toLowerCase();\n            return lesson.subject.toLowerCase().includes(query) ||\n                   lesson.location.toLowerCase().includes(query) ||\n                   lesson.price.toString().includes(query) ||\n                   lesson.spaces.toString().includes(query);\n          });\n\n          // Sorting logic based on sortKey and sortOrder\n          return filtered.sort((a, b) => {\n            let result;\n            if (this.sortKey === 'price' || this.sortKey === 'spaces') {\n              result = a[this.sortKey] - b[this.sortKey];\n            } else {\n              result = a[this.sortKey].localeCompare(b[this.sortKey]);\n            }\n            return this.sortOrder === 'asc' ? result : -result;\n          });\n        },\n        isCheckoutValid() {\n          // Check if the name contains only letters and phone contains only numbers\n          const nameValid = /^[A-Za-z\\s]+$/.test(this.name);\n          const phoneValid = /^[0-9]+$/.test(this.phone);\n          return nameValid && phoneValid && this.cart.length > 0;\n        }\n      },\n    mounted() {\n      this.fetchLessons();\n    }\n  };\n  </script>\n  "],"mappings":";;AAuCE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAAC,2CAA2C,CAAC;MACzE,IAAI,CAACJ,OAAM,GAAI,MAAMG,QAAQ,CAACE,IAAI,CAAC,CAAC;IACtC,CAAC;