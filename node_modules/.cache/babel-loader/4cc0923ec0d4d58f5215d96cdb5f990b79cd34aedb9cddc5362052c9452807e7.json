{"ast":null,"code":"export default {\n  data() {\n    return {\n      lessons: []\n    };\n  },\n  methods: {\n    async fetchLessons() {\n      const response = await fetch('http://localhost:3000/collections/products');\n      this.lessons = await response.json();\n    },\n    addToCart(lesson) {\n      if (lesson.spaces > 0) {\n        this.$emit('add-to-cart', lesson);\n        lesson.spaces--;\n      }\n    }\n  },\n  mounted() {\n    this.fetchLessons();\n  }\n};","map":{"version":3,"names":["data","lessons","methods","fetchLessons","response","fetch","json","addToCart","lesson","spaces","$emit","mounted"],"sources":["/Users/lyphilong/Desktop/FullStack/project/src/components/LessonList.vue"],"sourcesContent":["<template>\n    <div>\n      <h2>Available Lessons</h2>\n      <div v-for=\"lesson in lessons\" :key=\"lesson._id\">\n        <h3>{{ lesson.subject }}</h3>\n        <p>Location: {{ lesson.location }}</p>\n        <p>Price: ${{ lesson.price }}</p>\n        <p>Spaces Left: {{ lesson.spaces }}</p>\n        <button @click=\"addToCart(lesson)\" :disabled=\"lesson.spaces <= 0\">\n          Add to Cart\n        </button>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  export default {\n    data() {\n      return {\n        lessons: []\n      };\n    },\n    methods: {\n      async fetchLessons() {\n        const response = await fetch('http://localhost:3000/collections/products');\n        this.lessons = await response.json();\n      },\n      addToCart(lesson) {\n        if (lesson.spaces > 0) {\n          this.$emit('add-to-cart', lesson);\n          lesson.spaces--;\n        }\n      }\n    },\n    mounted() {\n      this.fetchLessons();\n    }\n  };\n  </script>\n  "],"mappings":"AAgBE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,YAAYA,CAAA,EAAG;MACnB,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAAC,4CAA4C,CAAC;MAC1E,IAAI,CAACJ,OAAM,GAAI,MAAMG,QAAQ,CAACE,IAAI,CAAC,CAAC;IACtC,CAAC;IACDC,SAASA,CAACC,MAAM,EAAE;M