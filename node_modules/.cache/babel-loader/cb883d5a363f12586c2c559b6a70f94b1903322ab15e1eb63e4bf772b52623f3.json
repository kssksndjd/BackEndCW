{"ast":null,"code":"export default {\n  props: ['cart'],\n  methods: {\n    removeFromCart(item) {\n      this.$emit('remove-from-cart', item);\n      item.spaces++;\n    },\n    checkout() {\n      // Simulate the checkout process\n      this.checkoutMessage = `Thank you, ${this.name}! Your order has been placed.`;\n      // Reset cart and form after checkout\n      this.cart = [];\n      this.name = '';\n      this.phone = '';\n      this.showCart = false; // Go back to lessons after successful checkout\n    },\n    async checkout() {\n      const order = {\n        items: this.cart,\n        name: 'John Doe',\n        phone: '1234567890'\n      };\n      await fetch('/api/orders', {\n        method: 'POST',\n        headers: {\n          'Content-Type': 'application/json'\n        },\n        body: JSON.stringify(order)\n      });\n      alert('Order placed successfully!');\n    }\n  },\n  compute: {\n    isCheckoutValid() {\n      // Check if the name contains only letters and phone contains only numbers\n      const nameValid = /^[A-Za-z\\s]+$/.test(this.name);\n      const phoneValid = /^[0-9]+$/.test(this.phone);\n      return nameValid && phoneValid && this.cart.length > 0;\n    }\n  }\n};","map":{"version":3,"names":["props","methods","removeFromCart","item","$emit","spaces","checkout","checkoutMessage","name","cart","phone","showCart","order","items","fetch","method","headers","body","JSON","stringify","alert","compute","isCheckoutValid","nameValid","test","phoneValid","length"],"sources":["/Users/lyphilong/Desktop/FullStack/project/src/components/ShoppingCart.vue"],"sourcesContent":["<template>\n  <div class=\"container mt-5\">\n    <div>\n      <h2 class=\"mt-5\">Shopping Cart</h2>\n      <ul class=\"list-group mb-4\">\n        <li v-for=\"item in cart\" :key=\"item._id\" class=\"list-group-item\">\n          <i class=\"fas fa-chalkboard-teacher\"></i> \n          {{ item.subject }} - ${{ item.price }} ({{ item.location }})\n          <button class=\"btn btn-danger btn-sm float-right\" @click=\"removeFromCart(item)\">Remove</button>\n        </li>\n      </ul>\n      <p v-if=\"cart.length === 0\" class=\"text-muted\">Your cart is empty.</p>\n\n    </div>\n\n    <h3>Checkout</h3>\n      <div v-if=\"cart.length > 0\">\n        <div class=\"form-group\">\n          <label for=\"name\">Name</label>\n          <input type=\"text\" id=\"name\" class=\"form-control\" v-model=\"name\" placeholder=\"Enter your name\">\n        </div>\n        <div class=\"form-group\">\n          <label for=\"phone\">Phone</label>\n          <input type=\"text\" id=\"phone\" class=\"form-control\" v-model=\"phone\" placeholder=\"Enter your phone number\">\n        </div>\n        <button class=\"btn btn-success\" :disabled=\"!isCheckoutValid\" @click=\"checkout\">Submit Order</button>\n        <p v-if=\"checkoutMessage\" class=\"alert alert-info mt-3\">{{ checkoutMessage }}</p>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  export default {\n    props: ['cart'],\n    methods: {\n      removeFromCart(item) {\n        this.$emit('remove-from-cart', item);\n        item.spaces++;\n      },\n      checkout() {\n          // Simulate the checkout process\n          this.checkoutMessage = `Thank you, ${this.name}! Your order has been placed.`;\n          // Reset cart and form after checkout\n          this.cart = [];\n          this.name = '';\n          this.phone = '';\n          this.showCart = false;  // Go back to lessons after successful checkout\n        },\n      async checkout() {\n        const order = {\n          items: this.cart,\n          name: 'John Doe',\n          phone: '1234567890'\n        };\n        await fetch('/api/orders', {\n          method: 'POST',\n          headers: { 'Content-Type': 'application/json' },\n          body: JSON.stringify(order)\n        });\n        alert('Order placed successfully!');\n      }\n    },\n    compute: {\n      isCheckoutValid() {\n          // Check if the name contains only letters and phone contains only numbers\n          const nameValid = /^[A-Za-z\\s]+$/.test(this.name);\n          const phoneValid = /^[0-9]+$/.test(this.phone);\n          return nameValid && phoneValid && this.cart.length > 0;\n        }\n    }\n  };\n  </script>\n  "],"mappings":"AAgCE,eAAe;EACbA,KAAK,EAAE,CAAC,MAAM,CAAC;EACfC,OAAO,EAAE;IACPC,cAAcA,CAACC,IAAI,EAAE;MACnB,IAAI,CAACC,KAAK,CAAC,kBAAkB,EAAED,IAAI,CAAC;MACpCA,IAAI,CAACE,MAAM,EAAE;IACf,CAAC;IACDC,QAAQA,CAAA,EAAG;MACP;MACA,IAAI,CAACC,eAAc,GAAI,cAAc,IAAI,CAACC,IAAI,+BAA+B;MAC7E;MACA,IAAI,CAACC,IAAG,GAAI,EAAE;MACd,IAAI,CAACD,IAAG,GAAI,EAAE;MACd,IAAI,CAACE,KAAI,GAAI,EAAE;MACf,IAAI,CAACC,QAAO,GAAI,KAAK,EAAG;IAC1B,CAAC;IACH,MAAML,QAAQA,CAAA,EAAG;MACf,MAAMM,KAAI,GAAI;QACZC,KAAK,EAAE,IAAI,CAACJ,IAAI;QAChBD,IAAI,EAAE,UAAU;QAChBE,KAAK,EAAE;MACT,CAAC;MACD,MAAMI,KAAK,CAAC,aAAa,EAAE;QACzBC,MAAM,EAAE,MAAM;QACdC,OAAO,EAAE;UAAE,cAAc,EAAE;QAAmB,CAAC;QAC/CC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAACP,KAAK;MAC5B,CAAC,CAAC;MACFQ,KAAK,CAAC,4BAA4B,CAAC;IACrC;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,eAAeA,CAAA,EAAG;MACd;MACA,MAAMC,SAAQ,GAAI,eAAe,CAACC,IAAI,CAAC,IAAI,CAAChB,IAAI,CAAC;MACjD,MAAMiB,UAAS,GAAI,UAAU,CAACD,IAAI,CAAC,IAAI,CAACd,KAAK,CAAC;MAC9C,OAAOa,SAAQ,IAAKE,UAAS,IAAK,IAAI,CAAChB,IAAI,CAACiB,MAAK,GAAI,CAAC;IACxD;EACJ;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}